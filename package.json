{
  "name": "allsafe-website",
  "version": "1.0.0",
  "description": "AllSafe Cybersecurity Services Website with Node.js Backend",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:3003": "set PORT=3003 && nodemon server.js",
    "kill-ports": "netstat -ano | findstr :3001 && netstat -ano | findstr :3002 && netstat -ano | findstr :3003",
    "setup-mysql": "node setup-mysql.js",
    "test-db": "node test-db.js"
  },
  "keywords": [
    "cybersecurity",
    "website",
    "nodejs",
    "express"
  ],
  "author": "AllSafe Team",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mysql2": "^3.14.3",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
